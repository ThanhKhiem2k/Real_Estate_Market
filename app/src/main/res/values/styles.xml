    <style name="FirebaseUI.Button.AccountChooser.icons8signout">
        <item name="drawableStartCompat">@drawable/icons8logout64</item>
        <item name="android:background">#A576C6</item>
        <item name="android:textColor">#FFFFFFFF</item>
